	"graph":{
		"prefix":"graph",
		"body":[
			"\\begin{figure}[H] %figure环境，可于此处加[h]来固定位置。",
			"\t\\centering  %使图片居中显示",
			"\t\\includegraphics[scale=0.5]{$1.png} %图片尺寸及位置。",
			"\t\\caption{$2} %图题",
			"\t\\label{$3} %标签，方便在文章中引用图片。",
			"\\end{figure} %figure环境$0",

		],
		"description": " 添加图片 "
	},
	"multigraph":{
		"prefix":"multigraph",
		"body":[
			"\\begin{figure}[H] %figure环境，可于此处加[h]来固定位置。",
			"\t\\centering  %使图片居中显示",
			"\t\\subfigure[$1]{ % 填子图图题",
			"\t\t\\includegraphics[scale=0.5]{$2.png}",
			"\t}",
			"\t\\quad",
			"\t\\subfigure[$3]{",
			"\t\t\\includegraphics[scale=0.5]{$4.png}",
			"\t}",
			"\t\\caption{$5} %图题",
			"\t\\label{$6} %标签，方便在文章中引用图片。",
			"\\end{figure}",
		],
		"description": " 多张图片 "
	},
	"addgraph":{
		"prefix":"addgraph",
		"body":[
			"\\quad",
			"\\subfigure[$1]{",
			"\t\\includegraphics[scale=0.5]{$2.png}",
			"}",
		],
		"description": " 添加图片 "
	},
	"sec":{
		"prefix":"sec",
		"body":[
			"\\section{$1}$0"
		],
		"description": " section "
	},
	"para":{
		"prefix":"para",
		"body":[
			"\\paragraph{$1} $0"
		],
		"description": " paragraph "
	},
	"algorithm":{
		"prefix":"algorithm",
		"body":[
			"\\begin{algorithm}[H]",
			"\t\\caption{$4}",
			"\t\\label{$5}",
			"\t\\begin{algorithmic}[1]",
			"\t\t\\Require $1 %输入",
			"\t\t\\Ensure $2  %输出",
			"\t\t\\Function {$3}{$1}",
			"\t\t\t\\State  %变量赋值",
			"\t\t\t\\If{}",
			"",
			"\t\t\t\\EndIf",
			"",
			"\t\t\t\\State \\Return{}",
			"\t\t\\EndFunction",
			"\t\\end{algorithmic}",
		"\\end{algorithm}"
		],
		"description": " algorithm "
	},
	
	"red":{
		"prefix":"red",
		"body":[
			"{\\color{red} $0}"
		],
		"description": " color red "
	},
	"blue":{
		"prefix":"blue",
		"body":[
			"{\\color{blue} $0}",
		],
		"description": " color blue "
	},
	"luotianyi":{
		"prefix":"luotianyi",
		"body":[
			"{\\color[HTML]{66CCFF} $0}",
		],
		"description": " color luotianyi "
	},

	"tianyibox":{
		"prefix":"tianyibox",
		"body":[
			"\\colorbox[HTML]{66CCFF}{",
			"\t\\vbox{",
			"\t\t$0",
			"\t}",
			"}"
		],
		"description": " tianyibox "
	},
	"bf":{
		"prefix":"bf",
		"body":[
			"\\textbf{$1}$0"
		],
		"description": " 加粗 "
	},
	"block":{
		"prefix":"block",
		"body":[
			"\\newtcolorbox{$1}{",
			"\tbreakable,",
			"\tfonttitle=\bfseries,",
			"\ttitle={$2}",
			"}",
			"\\begin{$1}",
			"$0",
			"\\end{$1}"
		],
		"description": " beautiful block "
	},
	
	
	
